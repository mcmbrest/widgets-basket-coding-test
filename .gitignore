# See https://github.com/github/gitignore/blob/main/Rails.gitignore
# This .gitignore is tailored for a typical Ruby on Rails application.

# Bundler
/.bundle/
/vendor/bundle/

# Logs and temp files
/log/*
!/log/.keep
/tmp/*
!/tmp/.keep
/tmp/pids/*
!/tmp/pids/
/tmp/cache/*
!/tmp/cache/
/tmp/sockets/*
!/tmp/sockets/

# Byebug / Pry history
/.byebug_history
/.pry_history

# dotenv environment variables
.env
.env.*
!.env.example

# Credentials and keys
/config/master.key
/config/credentials.yml.enc

# Storage (Active Storage)
/storage/*
!/storage/.keep

# Public uploads (if using CarrierWave, Shrine, etc.)
/public/uploads

# Assets generated by the asset pipeline or webpack(er)
/public/assets
/node_modules/
/yarn-error.log
/pnpm-lock.yaml

# Webpacker / Shakapacker packs
/public/packs
/public/packs-test

# Sprockets cache
/tmp/sprockets/*

# Bootsnap cache
/tmp/cache/bootsnap-*
/tmp/cache/bootsnap/*

# Spring
/tmp/spring/*

# SQLite databases
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-* 

# Coverage reports
/coverage/

# RuboCop
/.rubocop-https---*
/.rubocop_cache/

# RSpec / Capybara / System test artifacts
/spec/examples.txt
/tmp/capybara/*

# Bundler config
/.bundle/config

# IDE/editor files
/.idea/
/.vscode/
*.swp
*.swo
*~

# macOS
.DS_Store

# Node/npm logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Rails notes files
/annotate_models.log

# Rack-mini-profiler
/tmp/miniprofiler/*

# Zeitwerk / reloader dumps
/.zeitwerk-cache

# Terraform / Docker local overrides (if present)
terraform.tfstate*
.docker-local*

# Ignore local SSL certs often used by dev
/.ssl/
